/* pages/welcome/welcome.wxss */
.container{
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
}
.container .logo{
  padding:180rpx 0 80rpx 0;
}

.container .logo image{
  width:100rpx;
  height:100rpx;
}


.container .tre{
  width:300rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  padding:20rpx 50rpx;
  border:1px solid #333;
  margin-bottom: 28rpx;
  border-radius: 100rpx;
}
.container .tre text{
  font-size:32rpx;
}